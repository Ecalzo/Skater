{
    "manifest_version": 2,

    "name": "Dashboard",
    "description": "Spotlight-like feature for Chrome bookmarks",
    "version": "1.0",

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
            "default": "Ctrl+Shift+L",
            "mac": "MacCtrl+Shift+F"
            },
            "description":"Opens popup.html"
        }
    },
    "permissions": [
        "activeTab",
        "bookmarks"
    ]
}